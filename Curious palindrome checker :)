//checks both words and sentence
//though doesn't output a reversed sentence

#include <iostream>
#include <string>
using namespace std;

int main() {
    string palQ;
    string reversed;
    int len;
    cin >> palQ;
    len = palQ.length();
    
    for (int i = 0; i < len; i++) {
        reversed += palQ[len - 1 - i];
    }
    //cout << reversed << "\n";
    if (reversed == palQ) {
        cout << "Wow a palindrome !";
    }
    else {
        cout<< " Not really a palindrome ...";
    }
}
