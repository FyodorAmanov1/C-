#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    double x, y, z, x1, x2, sqD;
    cin >> x;
    cin >> y;
    cin >> z;

sqD = sqrt(pow(y,2) - 4 * x * z); 
x1 = (-y + sqD)/(2*x);
x2 = (-y - sqD)/(2*x);
if (x1 != x2) {
    cout << "x1 = " << x1 << " x2 = " << x2;
}
if (x1 == x2) {
    cout << "x1 = x2 = " << x2;
}

    return 0;
}

