#include <iostream>
using namespace std;

int main()
{
    int x;
    cin >> x;
    if (x < 0){
        cout << "Negative";
    } else {
        cout << "Positive";
    }

    return 0;
}

#include <iostream>
using namespace std;

int main()
{
    int x;
    cin >> x;
    if (x < 0 and x%2 == 0){
        cout << "The number is negative and even";
    } 
    if (x < 0 and x%2 == 1) { 
        cout << "The number is negative and odd";
    }
    if (x >= 0 and x%2 == 0){
        cout << "The number is positive and even";
    }
    if (x >= 0 and x%2 == 1){
        cout << "The number is positive and odd";
    }

    return 0;
}
