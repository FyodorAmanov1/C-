#include <iostream>
#include <string>
#include <cmath>
using namespace std;

int main()
{
   int n, t;
   cin >> t;
   string reply[t];
   for (int i = 0; i < t; ++i){
        cin >> n;
        int f21 = int(floor(n/2021));
        int f20 = int(floor(n/2020));
        if (n%2020%2021 == 0 or n%2021%2020 == 0 or n%2020 == 0 or n%2021 == 0 or (n - (f20 - 1)*2020)%2021 == 0 or (n - f21*2021)%2020 == 0) {
            reply[i] = "Yes";
        }
        else
            reply[i] = "No";   
   }
   
   for(int i = 0; i < t; ++i){
       cout << reply[i] << endl;
   }
    return 0;
}
